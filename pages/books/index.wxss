.container{
  
}
.sub-header{
  display: flex;
  justify-content: center;
}
.sub-header image{
 width: 107rpx;
 height: 34rpx;
 margin-top:70px 
}
.books-container{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 0 90rpx;
  justify-content: space-between;
}
.books-container v-book{
  margin-top:10rpx;
}
.header-container{
  display: flex;
  flex-direction: row;
  justify-content: center;

}
.header{
  border-top:1px solid #f5f5f5;
  border-bottom:1px solid #f5f5f5;
  box-shadow:0 0 3px 0 #e3e3e3;
  width: 100%;
  position: fixed;
  z-index: 99;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
 height: 50px;
 background: #fff;

}
.header image{
  width: 28rpx;
  height:28rpx;
}
.search{
  width: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
  background:#f5f5f5;
  height: 34px;
 border-radius: 50px;
}
